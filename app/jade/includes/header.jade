include ../mixins/mixins
.header
    .header_first
        .header_first_container
            +social(['fb', 'vk', 'twitter'], 'social-head')
            .head-contacts
                a(href="https://www.google.com.ua/maps/place/%D0%9C%D0%B0%D1%8F%D0%BA%D0%BE%D0%B2%D1%81%D0%BA%D0%B0%D1%8F+%D1%83%D0%BB.,+6,+%D0%97%D0%B0%D0%BF%D1%80%D1%83%D0%B4%D0%BD%D1%8F,+%D0%9C%D0%BE%D1%81%D0%BA%D0%BE%D0%B2%D1%81%D0%BA%D0%B0%D1%8F+%D0%BE%D0%B1%D0%BB.,+%D0%A0%D0%BE%D1%81%D1%81%D0%B8%D1%8F,+141960/@56.566418,37.4448678,17z/data=!3m1!4b1!4m2!3m1!1s0x46b4f616d72de7d3:0x4ff4f3fa5bc24009?hl=ru" target="_blank").head-contacts_loc
                    .head-contacts_loc_icon
                    .head-contacts_loc_content ул. Маяковская 6, офис 17
                a(href="tel:+79211231213").head-contacts_tel
                    .head-contacts_tel_icon
                    .head-contacts_tel_content +7 (921) 123-12-13
    .header_second
        .header_second_container
            a(href="/").logo
                img(src="img/logo-head.png", alt="cooltech").logo_img
            form(action="#" method="GET").search
                input(type="text" placeholder="Что будем искать").search_field
                input(type="submit" value="Найти").search_button
            +list(['Карта проезда', 'История заказов', 'Корзина'], 'header-nav')
            
            
        